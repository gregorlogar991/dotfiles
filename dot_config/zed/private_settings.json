{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "version": "2"
  },
  "vim_mode": true,
  "cursor_blink": false,
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Mocha",
    "dark": "One Dark"
  },
  "terminal": {
    "font_family": "Hack Nerd Font Mono",
    "font_size": 14
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_install_extensions": {
    "basher": true,
    "catppuccin": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "gosum": true,
    "groovy": true,
    "helm": true,
    "html": true,
    "java": true,
    "make": true,
    "mermaid": true,
    "nginx": true,
    "proto": true,
    "pylsp": true,
    "rainbow-csv": true,
    "terraform": true
  },
  "lsp": {
    "jdtls": {
      "initialization_options": {
        "bundles": [],
        "workspaceFolders": ["file:///home/snjeza/Project"],
        "settings": {
          "java": {
            "home": "Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home"
          }
        }
      }
    },
    "terraform-ls": {
      "initialization_options": {
        "experimentalFeatures": {
          "prefillRequiredFields": true
        }
      }
    }
  },
  "languages": {
    "Markdown": {
      "format_on_save": "on"
    }
  }
}
